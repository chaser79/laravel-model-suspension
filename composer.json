{
    "name": "digikraaft/laravel-model-suspension",
    "description": "A package to enable suspension of Eloquent Model",
    "keywords": [
        "digikraaft",
        "laravel-suspension"
    ],
    "homepage": "https://github.com/digikraaft/laravel-model-suspension",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Oladoyinbo",
            "email": "hello@digikraaft.ng",
            "homepage": "https://digikraaft.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0|^12.0",
        "spatie/test-time": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Digikraaft\\ModelSuspension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digikraaft\\ModelSuspension\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digikraaft\\ModelSuspension\\ModelSuspensionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
